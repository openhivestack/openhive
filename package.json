  "scripts": {
    "dev": "infisical run --path /common -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "ui": "npx shadcn@latest",
    "commit": "git add . && cz && git pull --rebase && git push",
    "generate": "infisical run --path /common -- prisma generate",
