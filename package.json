    "update": "infisical run --path /common -- ts-node prisma/update.ts",
    "seed": "infisical run --path /common -- ts-node prisma/seed.ts",
    "tf:init": "terraform -chdir=terraform init",
    "tf:plan": "infisical run --path /common -- terraform -chdir=terraform plan",
    "tf:apply": "infisical run --path /common -- terraform -chdir=terraform apply",
    "tf:destroy": "infisical run --path /common -- terraform -chdir=terraform destroy"
  },
